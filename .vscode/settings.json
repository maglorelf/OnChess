{
    "cSpell.words": [
        "AGAX",
        "FICS",
        "INCUDE",
        "noopener",
        "noreferrer",
        "Palaestra"
    ],
    "editor.formatOnSave": true,
    "editor.defaultFormatter": null,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
